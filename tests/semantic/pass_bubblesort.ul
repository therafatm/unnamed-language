void main (){
	int[5] array;
	array[0] = 1;
	array[1] = 123;
	array[2] = 23;
	array[3] = 67;
	array[4] = 0;

	bubblesort(array);
}

void bubblesort(int [5] array){
	int i;
	int j;
	int temp;

	i = 5;
	j = 5;
	while (0 < i-1) {
		while (0 < j-1) {
			if (array[i+1] < array[i]) {
				temp = array[i];
				array[i] = array[i+1];
				array[i+1] = temp;
				temp = 0;
			}
			j = j + 1;
		}
		i = i + 1;
	}
}
