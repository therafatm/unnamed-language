int add(int a, int b)
{
    return a + b;
}

float mul(float a, float b)
{
    return a * b;
}

float factorial(float n)
{
	float returnValue;

	if (n == 1)
	{
		returnValue = 1;
	}
	else
	{
		returnValue =  n*factorial(n-1);
	}
	return returnValue;
}

void main ()
{
    println add(2, 3);
    println mul(2.1, 3.5);
    print "Factorial of 3 is ";
    println factorial(3);
}